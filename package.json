{
  "name": "support-desk-app",
  "version": "1.0.0",
  "engines": {
    "node": "16.17.1",
    "npm": "8.15.0"
  },
  "description": "ticket support desk app",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
